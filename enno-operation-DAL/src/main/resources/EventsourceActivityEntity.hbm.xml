<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="enno.operation.dal.EventsourceActivityEntity" table="eventsource_activity" schema=""
           catalog="enno_operationserverdb">
        <id name="id">
            <column name="Id" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="value">
            <column name="Value" sql-type="varchar" length="200"/>
        </property>
    </class>
</hibernate-mapping>