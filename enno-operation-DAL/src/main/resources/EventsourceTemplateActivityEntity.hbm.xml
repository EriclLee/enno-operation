<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="enno.operation.dal.EventsourceTemplateActivityEntity" table="eventsource_template_activity" schema=""
           catalog="enno_operationserverdb">
        <id name="id">
            <column name="Id" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar" length="45"/>
        </property>
        <property name="displayName">
            <column name="DisplayName" sql-type="varchar" length="45"/>
        </property>
        <property name="comments">
            <column name="Comments" sql-type="varchar" length="200"/>
        </property>
        <property name="allowNull">
            <column name="AllowNull" sql-type="int unsigned"/>
        </property>
    </class>
</hibernate-mapping>